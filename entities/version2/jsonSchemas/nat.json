{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "nat",
    "extends": {
        "$ref": "./verigraph.json/#/definitions/networkFunction"
    },
    "$comment": "A nat configuration holds a set of private addresses that represents the hosts in the internal network",
    "type": "object",
    "properties": {
        "sources": {
            "type": "array",
            "items": {
                "type": "string",
                "pattern": "^([0-9]|([1-9][0-9])|(1[0-9][0-9])|(2[0-4][0-9])|(25[0-5]))([.]([0-9]|([1-9][0-9])|(1[0-9][0-9])|(2[0-4][0-9])|(25[0-5]))){3}$"
            }
        }
    }
}